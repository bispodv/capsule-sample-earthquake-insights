capsule {
  id (example.earthquakeInsights)
  version (0.1.2)
  format (3)
  targets {
    target (bixby-mobile-en-US)
  }
  capsule-imports {
    import (viv.time) {
      as (time) version (2.88.10)     
    }
    import (viv.geo) {
      as (geo) version (9.17.23)
    }
  }
  permissions {
    user-profile-access
  }
  runtime-flags {
    concepts-inherit-super-type-features
    modern-prompt-rejection
    support-halt-effect-in-computed-inputs
  }
}
