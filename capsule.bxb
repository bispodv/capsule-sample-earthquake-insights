capsule {
	id (example.earthquakeInsights) 
	version (0.1.2)
	format (3)
	targets {
		target (bixby-mobile-en-US)
	}
	capsule-imports {
		import (viv.time) {
			as (time) version (3.3.34)
		}
		import (viv.geo) {
			as (geo) version (9.27.15)
		}
	}
	permissions {
		device-location-access
	}
	runtime-version (8) {
		overrides {
			no-fallback-dialog-in-views (false)
		}
	}
	store-countries {
		all
	}
	store-sections {
		section (EducationAndReference)
	}
}
